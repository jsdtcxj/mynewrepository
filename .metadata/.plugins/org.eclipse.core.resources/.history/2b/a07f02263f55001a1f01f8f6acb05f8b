	private int[] ans = new int[2];
	private boolean matched = false;
	public int[] fin() {
		//0: blue, 1: red
		int[] A = new int[2];
		int[] B = new int[2];
		int[] C = new int[3];
		search(0,0,0,new LinkedList());
		return ans;
	}
	
	public void search(int countBlue, int countRed, int start, LinkedList<Integer> list) {
		if(start == 3) {
			matched = isValid(list);
			if(matched) {
				ans[0] = list.get(0);
				ans[1] = list.get(1);
			}
		} else {
			if(countBlue < 2) {
				list.add(0);
				list.add(0);
				search(countBlue+2, countRed, start+1, list);
				list.removeLast();
				list.removeLast();
			}
			if(countRed < 2) {
				list.add(1);
				list.add(1);
				search(countBlue, countRed+2, start+1, list);
				list.removeLast();
				list.removeLast();
			}
			if(countBlue < 1 && countRed < 1) {
				list.add(0);
				list.add(1);
				search(countBlue+1, countRed+1, start+1, list);
				list.removeLast();
				list.removeLast();
			}
		}
	}
	
	public boolean isValid(LinkedList<Integer> list) {
		int sum = 0;
		for(int num : list) {
			sum += num;
		}
		return (sum == 4 || sum == 0);
	}
 }
