	private int count = 1;
	public int uniquePaths(int m, int n) {
	    boolean[][] memo = new boolean[m+1][n+1];
	    search(memo, 1, 1, m, n);
	    return count;
	}

	public void search(boolean[][]memo, int i, int j, int m, int n) {
	    if(!memo[i][j]) {
	        memo[i][j] = true;
	        if(i < m && j < n) {
	            count++;
	        }
	        if(i < m) {
	            search(memo, i+1, j, m, n);
	        }
	        if(j < n) {
	            search(memo, i, j+1, m, n);
	        }
	    }
	}

 }
