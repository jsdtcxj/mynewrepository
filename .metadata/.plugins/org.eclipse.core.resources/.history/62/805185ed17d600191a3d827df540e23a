	//[[10,20],[30,200],[400,50],[30,20]]
	public int twoCitySchedCost(int[][] costs) {
        int len = costs.length, sum = 0;
        int[] offset = new int[len];
        for(int i = 0; i < len; i++) {
            int a = costs[i][0], b = costs[i][1];
            sum += a;
            offset[i] = b-a;
        }
        Arrays.sort(offset);
        for(int i = 0; i < len/4; i++) {
            sum += offset[i];
        }
        return sum;
    }
}
