	public List<Integer> grayCode(int n) {
        List[] dp = new List[n+1];
        List<Integer> list = new ArrayList();
        list.add(0);
        dp[0] = list;
        for(int i = 1; i <= n; i++) {
            List<Integer> list1 = dp[n-1];
            list = new ArrayList();
            int max = 0;
            for(int j = 0; j < list1.size(); j++) {
                int num = list1.get(j);
                max = Math.max(max, num);
                list.add(num);
            }
            max++;
            for(int j = list1.size()-1; j >= 0; j--) {
                list.add(list1.get(j)+max);
            }
            dp[n] = list;
        }
        return dp[n];
    }

 }
