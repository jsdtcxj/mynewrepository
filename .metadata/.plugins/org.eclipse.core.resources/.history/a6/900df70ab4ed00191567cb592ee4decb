	public boolean canJump(int[] nums) {
        if(nums == null || nums.length < 2) return true;
        int len = nums.length;
        boolean[] dp = new boolean[len];
        dp[len-1] = true;
        for(int i = len-2; i >= 0; i--) {
            int step = nums[i];
            for(int j = 1; j <= step; j++) {
                if(i+j < len && dp[i+j]) {
                    dp[i] = true;
                    break;
                }
            }
        }
        return dp[0];
    }
 }
