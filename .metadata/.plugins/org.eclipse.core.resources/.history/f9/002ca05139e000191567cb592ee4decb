	public List<List<Integer>> combine(int n, int k) {
	    List<List<Integer>> ans = new ArrayList();
	    search(ans, new ArrayList<Integer>(), k, 1, n);
	    return ans;
	}

	public void search(List<List<Integer>> ans, List<Integer> list, int k, int start, int end) {
	    int size = list.size();
	    if(size == k) {
	        ans.add(list);
	    } else {
	        for(int i = start; i <= end; i++) {
	            List<Integer> list1 = new ArrayList();
	            list1.addAll(list);
	            list1.add(i);
	            search(ans, list1, k, i+1, end);
	        }
	    }
	}

 }
