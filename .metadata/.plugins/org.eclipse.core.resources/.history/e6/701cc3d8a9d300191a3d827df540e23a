	public boolean queryString(String S, int N) {
        int temp = 1;
        while(temp < N) {
            temp <<= 1;
        }
        int start = 0;
        if(N == temp) {
            start = temp>>1;
        } else {
            start = temp>>2;
        }
        for(int i = start; i<=N; i++) {
            if(S.indexOf(toBinary(i)) < 0) {
                return false;
            }
        }
        return true;
    }
    
    public String toBinary(int N) {
        String binary = "";
        while(N > 0) {
            if(N%2 == 0) {
                binary = "0"+binary;
                N = N/2;
            } else {
                binary = "1"+binary;
                N = (N-1)/2;
            }
        }
        return binary;
    }
}
