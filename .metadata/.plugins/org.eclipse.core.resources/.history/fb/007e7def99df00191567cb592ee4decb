	public int mySqrt(int x) {
	       int temp = 1;
	        while(temp * temp < x) {
	            temp <<= 1;
	        }
	        
	        int start = temp/2, end = temp;
	        while(start < end) {
	            int mid = (start+end)/2;
	            if(mid*mid < x) {
	                start = mid+1;
	            } else {
	                end = mid;
	            }
	        }
	        return (start*start > x) ? start-1 : start;
	    }

 }
