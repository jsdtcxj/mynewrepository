	public List<Integer> grayCode(int n) {
        List<Integer> list = new ArrayList();
        list.add(0);
        for(int i = 1; i <= n; i++) {
            int add = (i == 1) ? 1 : i<<1;
            int len = list.size();
            for(int j = len-1; j >= 0; j--) {
                list.add(list.get(j)+add);
            }
        }
        return list;
    }

 }
