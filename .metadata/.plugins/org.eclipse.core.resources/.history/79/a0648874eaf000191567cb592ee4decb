	public ListNode reverseBetween(ListNode head, int m, int n) {
        ListNode dummy = new ListNode(-1);
        dummy.next = head;
        n -= m;
        while(m > 1) {
            head = head.next;
            m--;
        }
        ListNode prev = (m > 1) ? head : dummy;
        ListNode first = head.next;
        ListNode second = first.next;
        ListNode third = null;
        while(n > 0) {
            if(second != null) {
                System.out.println((third != null) ? third.val : "null" + ", " + second.val + ", " + first.val);
                third = second.next;
                second.next = first;
                first = second;
                second = third;
            }
            n--;
        }
        prev.next.next = second;
        prev.next = first;
        return dummy.next;
    }
	 
	public class ListNode {
	      int val;
	     ListNode next;
	     ListNode(int x) { val = x; }
	 }
 }
