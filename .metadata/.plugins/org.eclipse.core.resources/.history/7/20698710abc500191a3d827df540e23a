	public boolean canJump(int[] nums) {
        int len = nums.length;
        if(len == 1) return true;

        int[] memo = new int[len];
        memo[len-1] = 1;
        return backtrace(nums, memo, 0);
    }
	
	public boolean backtrace(int[] nums, int[] memo, int position) {
        if(memo[position] == 1) {
            return true;
        } else if(memo[position] == -1) {
            return false;
        } else {
            int total = Math.max(position+nums[position], nums.length-1);
            for(int j = total; j > position; j--) {
                if(backtrace(nums, memo, total)) {
                    return true;
                } else {
                    memo[total] = -1;
                }
            }
            memo[position] = -1;
            return false;
        }

    }
}
