	public boolean search(int[] nums) {
	    return (nums == null || nums.length == 0) ? true : search(nums, null, 0, nums.length-1);
	}

	public boolean search(int[] nums, Integer parent, int start, int end) {
	    if(start >= end) return true;
	    int num = nums[end];
	    if(parent != null) {
	        for(int i = start; i <= end; i++) {
	            if(nums[i] >= parent) return false;
	        }
	    }
	    int index = end-1;
	    while(index >= start && nums[index] > num) {
	        index--;
	    }
	    return search(nums, null, index+1, end) && search(nums, num, start, index);
	}
 }
