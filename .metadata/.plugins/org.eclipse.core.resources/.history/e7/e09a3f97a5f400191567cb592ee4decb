	 public List<List<Integer>> generate(int numRows) {
	        List<List<Integer>> ans = new ArrayList();
	        for(int i = 0; i < numRows; i++) {
	            List<Integer> list = new ArrayList();
	            ans.add(list);
	            for(int j = 0; j < i; j++) {
	                int num = 1;
	                if(j > 0 && j < i-1) {
	                    List<Integer> prev = ans.get(i-1);
	                    num = prev.get(j) + prev.get(j-1);
	                }
	                list.add(num);
	            }
	        }
	        return ans;
	    }
 }
