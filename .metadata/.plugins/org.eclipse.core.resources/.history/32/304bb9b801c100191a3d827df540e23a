	public double myPow(double x, int n) {
        double ans = 1.0;
        boolean isMin = n == Integer.MIN_VALUE;
        int num = isMin ? Integer.MAX_VALUE : Math.abs(n);
        for(int i = 0; i < num; i++) {
            ans *= x; 
        }
        if(isMin) ans *= x;
        if(x < 0) {
            ans = 1.0 / ans;
        }
        return ans;
    }
}
