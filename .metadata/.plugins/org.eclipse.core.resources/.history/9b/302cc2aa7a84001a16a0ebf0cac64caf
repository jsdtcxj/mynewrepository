package com.chs.coreJava.repeatable.annotation;

import java.lang.annotation.Annotation;

@Hint("hint1")
@Hint("hint2")
public class Test {

	public static void main(String[] args) {
		Hints hints = Person.class.getAnnotation(Hints.class);
		for(Hint h : hints.value()) {
			System.out.println(h.value());
		}

		Annotation[] aa = Test.class.getAnnotations();
		for (Annotation a : aa) {
			System.out.println(a);
		}

		Hint[] hints2 = Test.class.getAnnotationsByType(Hint.class);
		for (Hint h : hints2) {
			System.out.println(h.value());
		}

	}

}
