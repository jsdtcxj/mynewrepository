	public int bitwiseComplement(int N) {
        int count = 0, temp = 1, ans = 0;
        if(N == 0) return 1;
        while(N > temp) {
            temp <<= 1;
            count++;
        }

        StringBuilder sb = new StringBuilder();
        while(N > 0 || temp > 0) {
            if(N >= temp) {
                N -= temp;
                sb.append("1");
            } else {
                if(sb.length() > 0) {
                    sb.append("0");
                }
            }
            temp >>= 1;
            count--;

        }

        temp = 1;
        for(int i = sb.length()-1; i >= 0; i--) {
            char c = sb.charAt(i);
            if(c == '0') {
                ans += temp;
            }
            temp <<= 1;
        }
        return ans;
    }
}
