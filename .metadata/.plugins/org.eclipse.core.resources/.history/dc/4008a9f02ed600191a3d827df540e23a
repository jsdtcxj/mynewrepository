	//[[10,20],[30,200],[400,50],[30,20]]
	public int[] numMovesStones(int a, int b, int c) {
        int[] ans = new int[2];
        if(Math.abs(a-b) == 1 || Math.abs(c-b) == 1) {
            ans[0] = 1;
        } else {
            ans[0] = 2;
        }
        ans[1] = Math.abs(a-b-1) + Math.abs(c-b-1);
        return ans;
    }
}
