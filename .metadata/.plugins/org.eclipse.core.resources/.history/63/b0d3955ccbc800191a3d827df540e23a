	public int search(int[] nums, int target) {
	    if(nums == null || nums.length == 0) return -1;
        int start = 0, end = nums.length-1, mid = 0;
        while(start < end) {
            mid = (start+end)>>1;
        	if(start == mid) {
        		start = (nums[start] < nums[end]) ? start : end;
        		break;
        	}
            if(nums[start] > nums[mid]) {
                end = mid;
            } else {
                start = mid;
            }
        }
       
        int last = nums.length-1;
        if(start > 0) {
        	if(target >= nums[0] && target <= nums[start-1]) {
        		start = 0;
        		end = start-1;
        	} else if(target >= nums[start] && target <= nums[last]) {
        		end = last;
        	} else {
        		return -1;
        	}
        } else {
        	end = last;
        }
        
        while(start < end) {
        	mid = (start+end) >> 1;
        	if(target > nums[mid]) {
        		start = mid + 1;
        	} else {
        		end = mid;
        	}
        }
        
        return (target == nums[start]) ? start : -1;
	}
}
